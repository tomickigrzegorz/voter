{
  "name": "wybornik",
  "version": "1.0.0",
  "main": "dist/script.js",
  "description": "A simple way to select photos and their size. Useful for photographers.",
  "license": "MIT",
  "keywords": [
    "photo-selection",
    "photo-list",
    "photo"
  ],
  "scripts": {
    "dev": "concurrently \"yarn server\" \"yarn webpack:dev\"",
    "prod": "yarn webpack:prod",
    "webpack:dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js --progress",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress",
    "json": "node ./helpers/generate-json.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^8.1.1",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.2.14",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.3.0",
    "pug-loader": "^2.4.0",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
