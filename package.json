{
  "name": "wybornik",
  "version": "1.0.0",
  "main": "dist/script.js",
  "description": "A simple way to select photos and their size. Useful for photographers.",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn server\" \"yarn watch\"",
    "prod": "rollup -c --environment NODE_ENV:PRODUCTION",
    "watch": "rollup -c -w",
    "json": "node ./helpers/generate-json.js",
    "server": "node server.js"
  },
  "keywords": [
    "photo-selection",
    "photo-list",
    "photo"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "json-server": "^0.16.3",
    "prettier": "^2.3.1",
    "rollup": "^2.50.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
